{
  "name": "nativephp-thinkphp",
  "version": "1.0.0",
  "description": "NativePHP for ThinkPHP",
  "main": "src/resources/electron/main.js",
  "scripts": {
    "dev": "electron .",
    "watch": "electron . --watch",
    "build": "electron-builder",
    "test": "pest",
    "lint": "eslint src/resources/electron/*.js",
    "analyze": "source-map-explorer src/resources/electron/main.js"
  },
  "keywords": [
    "nativephp",
    "thinkphp",
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-builder": "^24.0.0",
    "electron-updater": "^6.0.0",
    "electron-log": "^5.0.0",
    "terser": "^5.0.0",
    "clean-css-cli": "^5.0.0",
    "eslint": "^8.0.0",
    "source-map-explorer": "^2.5.3",
    "node-inspector": "^3.0.0",
    "chokidar": "^3.5.3",
    "jest": "^29.0.0",
    "jest-environment-node": "^29.0.0",
    "cross-env": "^7.0.3"
  },
  "build": {
    "appId": "com.nativephp.thinkphp",
    "productName": "NativePHP ThinkPHP",
    "directories": {
      "output": "build"
    },
    "files": [
      "src/resources/electron/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": ["dmg", "zip"]
    },
    "win": {
      "target": ["nsis", "zip"]
    },
    "linux": {
      "target": ["AppImage", "deb"]
    }
  }
}
