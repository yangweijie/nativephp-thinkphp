{
    "name": "yangweijie/native-thinkphp",
    "description": "thinkphp wrapper for the NativePHP framework.",
    "keywords": [
        "nativephp",
        "thinkphp",
        "nativephp-thinkphp"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0",
        "phpunit/phpunit": "^11.5",
        "symfony/finder": "^6.2|^7.0",
        "topthink/framework": "^8.0",
        "topthink/think-migration": "^3.1",
        "webmozart/assert": "^1.11",
        "yzh52521/easyhttp": "^1.1",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "native\\thinkphp\\": "src/"
        },
        "files": [
            "src\\helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {

        }
    },
    "scripts": {

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "think": {
            "services": [
                "native\\thinkphp\\NativeService"
            ]
        },
        "laravel": {
            "providers": [
              "Native\\Laravel\\NativeServiceProvider"
            ],
            "aliases": {
                "ContextMenu": "Native\\Laravel\\Facades\\ContextMenu",
                "Dock": "Native\\Laravel\\Facades\\Dock",
                "Process": "Native\\Laravel\\Facades\\Process",
                "Window": "Native\\Laravel\\Facades\\Window",
                "Clipboard": "Native\\Laravel\\Facades\\Clipboard"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
