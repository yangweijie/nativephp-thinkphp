{
    "name": "nativephp/thinkphp",
    "description": "ThinkPHP的NativePHP扩展包",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NativePHP\\Think\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "NativePHP\\Think\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "james.xue",
            "email": "1527736751@qq.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "topthink/framework": "^8.0",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "pestphp/pest": "^3.7"
    },
    "extra": {
        "think": {
            "services": [
                "NativePHP\\Think\\NativeServiceProvider"
            ],
            "config": {
                "native": "src/config/native.php"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
